{
  "name": "babel-plugin-transform-react-pure-to-component",
  "version": "1.0.1",
  "description": "Convert React.PureComponent to React.Component to improve hot reloading.",
  "main": "index.js",
  "author": "Chris Sauve <chrismsauve@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check": "yarn run lint && yarn run test",
    "prebuild": "rimraf lib",
    "build": "babel src --out-dir lib",
    "prepublish": "yarn run build"
  },
  "eslintConfig": {
    "extends": [
      "plugin:shopify/esnext",
      "plugin:shopify/node"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>/test"
    ],
    "testRegex": "[\\w+]\\.test\\.js$",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-plugin-shopify": "^15.1.1",
    "fs-extra": "^2.0.0",
    "jest": "^18.1.0",
    "rimraf": "^2.5.4"
  }

,"_resolved": "https://registry.npmjs.org/babel-plugin-transform-react-pure-to-component/-/babel-plugin-transform-react-pure-to-component-1.0.1.tgz"
,"_integrity": "sha1-KxiiCT2zRzNA6tDMqdnZROaX1qg="
,"_from": "babel-plugin-transform-react-pure-to-component@1.0.1"
}